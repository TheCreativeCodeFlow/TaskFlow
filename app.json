{
  "expo": {
    "name": "TaskFlow",
    "slug": "TaskFlow",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "dark",
    "scheme": "taskflow",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0D1117"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.taskflow.app"
    },
    "android": {
      "package": "com.taskflow.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#0D1117"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "POST_NOTIFICATIONS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/logo.png",
          "color": "#00F5FF",
          "mode": "production"
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2a334c01-c5a6-415e-8af0-9c00b467d392"
      }
    },
    "owner": "pixelcrafted2004"
  }
}
